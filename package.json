{
  "name": "jimple",
  "version": "2.0.0",
  "description": "jimple - a JS dependency injection container/service locator",
  "main": "src/index.js",
  "author": "Julien Bianchi <contact@jubianchi.fr>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/jubianchi/laboard.git"
  },
  "engines": {
    "node": "^4.0"
  },
  "devDependencies": {
    "atoum.js": "~0.0.11",
    "istanbul": "^0.3.20",
    "jscs": "^1.7.3",
    "mocha": "^2.3.2",
    "should": "^7.1.0"
  },
  "scripts": {
    "test": "mocha -s 1 --require should --reporter spec --check-leaks tests/spec/**/*.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- -s 1 --require should --reporter spec --check-leaks tests/spec/**/*.js",
    "cs": "node_modules/.bin/jscs src/ test/",
    "dtest": "docker run --rm -it -v $(pwd):/app -w /app jubianchi/docker-node-4 /node-v4.1.0-linux-x64/bin/npm run test",
    "dcover": "docker run --rm -it -v $(pwd):/app -w /app jubianchi/docker-node-4 /node-v4.1.0-linux-x64/bin/npm run cover",
    "dcs": "docker run --rm -it -v $(pwd):/app -w /app jubianchi/docker-node-4 /node-v4.1.0-linux-x64/bin/npm run cs"
  }
}
